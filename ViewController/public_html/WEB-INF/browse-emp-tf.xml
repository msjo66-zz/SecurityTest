<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="browse-emp-tf">
    <default-activity>browseEmp</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="browseEmp">
      <page>/browseEmp.jsff</page>
    </view>
    <view id="view1"></view>
    <control-flow-rule id="__1">
      <from-activity-id>browseEmp</from-activity-id>
      <control-flow-case id="__2">
        <to-activity-id>view1</to-activity-id>
      </control-flow-case>
    </control-flow-rule>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
